<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:NoodleCV.App.Views"
             xmlns:viewModels="clr-namespace:NoodleCV.App.ViewModels"
             xmlns:view="clr-namespace:NoodleCV.App.CustomControls.View"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NoodleCV.App.Views.ParameterView" x:DataType="viewModels:ParameterViewModel">

    <StackPanel Spacing="10" Margin="10">
        <Label Content="Selected Node Name" FontWeight="SemiBold" FontSize="24" />
        <StackPanel Orientation="Vertical">
            <Label Content="Label1" />
            <ComboBox HorizontalAlignment="Stretch">
                <ComboBoxItem Content="test" />
                <ComboBoxItem Content="test2" />
            </ComboBox>
        </StackPanel>

        <Separator />
        <views:CustomSlider />
        <Separator />
        <view:CustomFilePicker />
        <Separator />

        <StackPanel Orientation="Vertical">
            <Label Content="Label3" />
            <TextBox x:Name="test" />
        </StackPanel>
    </StackPanel>
</UserControl>